{
    "manifest_version": 3,
    "name": "JiraNest Extension",
    "description": "View Jira tasks and bugs directly from a Chrome sidebar.",
    "version": "1.0.0",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "storage",
      "identity",
      "tabs"
    ],
    "host_permissions": [
      "https://*.atlassian.net/*"
    ]
  }
  